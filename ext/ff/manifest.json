{
    "manifest_version": 2,
    "name": "Dalennod",
    "version": "1.0.0",
    "description": "A bookmark manager.",
    "icons": {
        "48": "icons/dalennod-48.png",
        "96": "icons/dalennod-96.png"
    },
    "homepage_url": "https://github.com/dalennod/dalennod",
    "permissions": [
        "tabs"
    ],
    "browser_action": {
        "default_title": "Dalennod",
        "default_popup": "add.html"
    },
    "content_security_policy": "script-src 'self' add.js; object-src 'self'"
}